	<!-- Start Hero Section -->
<div class="hero">
    <div class="container">
        <div class="row justify-content-between">
            <div class="col-lg-5">
                <div class="intro-excerpt">
                    <h1>My account</h1>
                </div>
            </div>
            <div class="col-lg-7">

            </div>
        </div>
    </div>
</div>
	<!-- End Hero Section -->
    <!-- Start Account Description -->
<div class="d-flex justify-content-center mt-5">

        <div class="fr-flbox">
            <h3 id="" class="fr-head h3">
                <span class="title">Profile</span>
            </h3>
            <div class="w4-f">
                <div class="mb-m profile-details">
                    <div class="fr-flbox column top">
                        <h4 id="" class="fr-head h4"><span class="title">Email</span></h4>
                        <span class="row-description mt-xxxs pr-xs" data-test="member-email">{{email}}</span>
                    </div>
                </div>
                <div class="mb-m profile-details">
                    <div class="fr-flbox column top">
                        <h4 id="" class="fr-head h4"><span class="title">Name</span></h4>
                        <span class="row-description mt-xxxs pr-xs" data-test="member-name">{{name}}</span>
                    </div>
                </div>
                <div class="mb-m profile-details">
                    <div class="fr-flbox column top">
                        <h4 id="" class="fr-head h4"><span class="title">Address</span></h4>
                        <span class="row-description mt-xxxs pr-xs" data-test="member-address">{{address}}</span>
                        </div>
                    </div>
                </div>
            <div class="w1-f"></div>
            <div class="w4-f">
                <div class="mb-m profile-details">
                    <div class="fr-flbox column top">
                        <h4 id="" class="fr-head h4"><span class="title">Contact</span></h4>
                        <span class="row-description mt-xxxs pr-xs" data-test="member-phone">{{contact}}</span>
                    </div>
                </div>
                <div class="mb-m profile-details">
                    <div class="fr-flbox column top">
                        <h4 id="" class="fr-head h4"><span class="title">Phone</span></h4>
                            <span class="row-description mt-xxxs pr-xs" data-test="member-mobile-phone">{{phone}}</span>
                    </div>
                </div>
                <div class="mb-m profile-details">
                    <div class="fr-flbox column top">
                        <h4 id="" class="fr-head h4"><span class="title">Date of birth</span></h4>
                        <span class="row-description mt-xxxs pr-xs" data-test="member-birthday">{{birthday}}</span>
                    </div>
                </div>
                <div class="mb-m profile-details">
                    <div class="fr-flbox column top">
                        <h4 id="" class="fr-head h4"><span class="title">Gender</span></h4>
                        <span class="row-description mt-xxxs pr-xs" data-test="member-gender">Nam</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>